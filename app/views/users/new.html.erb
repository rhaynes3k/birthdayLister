<h1> Sign Up Form </h1>
<%= form_tag("/users") do %>
<p>
<%= label_tag "Name:" %>
<%= text_field_tag "name" %></br>
<%= label_tag "Email:" %>
<%= text_field_tag "email" %><br>
<%= label_tag "Birthday:" %>
<%= text_field_tag "birthday" %>
<%= label_tag "Password:" %>
<%= text_field_tag "password" %>
</p>
<%= submit_tag "Sign Up" %>
<% end %>
